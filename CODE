#include<bits/stdc++.h>
#include<iostream>
using namespace std;
int max_arrival=-1,min_arrival=INT_MAX;              	 
struct Process
{
    	int pid=0;                                       
    	int priority=0;                                  
    	int arrival_time=0;                              
		int burst_time=0;                                
    	int completion_time=0;                           
    	int turnaround_time=0;                           
		int waiting_time=0;                              
    	int response_time=0;                             
		int remaining_time=0;                              
    	int CPUtime=-1;                                  //Stores When Process got CPU for first time
};
